{
  "name": "andkom/php-bitcoin-blockchain",
  "description": "A simple PHP library for reading Bitcoin blockchain database.",
  "type": "library",
  "keywords": [
    "bitcoin",
    "blockchain",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrei Komarov"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-leveldb": "^0.2.1",
    "stephenhill/base58": "^1.1",
    "bitwasp/bech32": "^0.0.1",
    "andkom/php-bcdatastream": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0"
  },
  "autoload": {
    "psr-4": {
      "AndKom\\PhpBitcoinBlockchain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndKom\\PhpBitcoinBlockchain\\Tests\\": "tests/"
    }
  }
}
